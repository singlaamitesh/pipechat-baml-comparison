# LLM Provider (gemini|openai)
LLM_PROVIDER=gemini

# OpenAI API Configuration (optional if using Gemini)
OPENAI_API_KEY=

# Google Gemini API Configuration (free tier available)
GOOGLE_API_KEY=

# ElevenLabs API Configuration (Text-to-Speech, optional/paid)
ELEVENLABS_API_KEY=

# Deepgram API Configuration (Speech-to-Text, optional/free tier)
DEEPGRAM_API_KEY=

# Pipechat Configuration
PIPECHAT_LOG_LEVEL=INFO

# BAML Configuration (optional)
BAML_API_KEY=

# Test/Output Configuration
TEST_MODE=true
METRICS_SAVE_PATH=./metrics/
